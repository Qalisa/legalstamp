---
import { availableFormatsConfig, formatStubs, TAG__LATEST } from "helpers/legalstamp.groupedBy";

export const prerender = false

const { slug } = Astro.params;
const [productOrOrganization, documentType, lang] = slug!.split('/')

//
const goTo = [formatStubs.dynamic, documentType, productOrOrganization, lang, TAG__LATEST, availableFormatsConfig.markdown.name].filter(Boolean).join('/')
return Astro.rewrite(goTo)
