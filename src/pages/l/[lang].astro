---
import ProductOrOrganization from '@components/ListBy/ProductOrOrganization.astro';
import { langFirst } from 'helpers/legalstamp.groupedBy'
import Layout from 'layouts/Layout.astro';

export const getStaticPaths = () =>
 Object.keys(langFirst).map(key => ({params: {lang: key}}))

const { lang } = Astro.params;
const [_, subItems] = Object.entries(langFirst)
    .find(([key,]) => key == lang) ?? []
const keys = Object.keys(subItems)    
---
<Layout>
    <ProductOrOrganization productsOrOrgs={keys}>
</Layout>