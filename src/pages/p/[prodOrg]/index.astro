---
import DocumentType from 'components/ListBy/DocumentType.astro';
import { successiveFilterByValues } from 'helpers/groupBySuccessive';
import { productOrOrgFirst } from 'helpers/legalstamp.groupedBy'
import Layout from 'layouts/Layout.astro';

export const getStaticPaths = () =>
 Object.keys(productOrOrgFirst).map(key => ({params: {prodOrg: key}}))

const { prodOrg } = Astro.params;
const keys = Object.keys(
    successiveFilterByValues(productOrOrgFirst, [prodOrg])
)   
---
<Layout>
    <DocumentType docTypes={keys}>
</Layout>