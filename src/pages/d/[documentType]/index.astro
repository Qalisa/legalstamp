---
import ProductOrOrganization from 'components/ListBy/ProductOrOrganization.astro';
import { successiveFilterByValues } from 'helpers/groupBySuccessive';
import { docFirst } from 'helpers/legalstamp.groupedBy'
import Layout from 'layouts/Layout.astro';

export const getStaticPaths = () =>
 Object.keys(docFirst).map(key => ({params: {documentType: key}}))

const { documentType } = Astro.params;

const keys = Object.keys(
    successiveFilterByValues(docFirst, [documentType])
)    
---
<Layout>
    <ProductOrOrganization productsOrOrgs={keys}>
</Layout>