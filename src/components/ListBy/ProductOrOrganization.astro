---
import Base from 'components/ListBy/_Base.astro'
interface Props {
  productsOrOrgs?: string[]
  originPathnameAgnostic?: boolean
}
const { productsOrOrgs, originPathnameAgnostic } = Astro.props

import { productOrOrgFirst } from 'helpers/legalstamp.groupedBy'
---
<Base  
  filter={productsOrOrgs}
  groupByResult={productOrOrgFirst}
  originPathnameAgnostic={originPathnameAgnostic}
  defaultPath="/p"
  dataName="productOrOrganization"
  description="By Product or Organization"
/>
