---
import Base from 'components/ListBy/_Base.astro'
interface Props {
  langs?: string[]
  originPathnameAgnostic?: boolean
}
const { langs, originPathnameAgnostic } = Astro.props

import { langFirst } from 'helpers/legalstamp.groupedBy'
---
<Base  
  filter={langs}
  groupByResult={langFirst}
  originPathnameAgnostic={originPathnameAgnostic}
  defaultPath="/lang"
  dataName="lang"
  description="By Lang"
/>
