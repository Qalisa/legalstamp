---
import Base from 'components/ListBy/_Base.astro'
interface Props {
  docTypes?: string[]
  originPathnameAgnostic?: boolean
}
const { docTypes, originPathnameAgnostic } = Astro.props

import { docFirst } from 'helpers/legalstamp.groupedBy'
---
<Base  
  filter={docTypes}
  groupByResult={docFirst}
  originPathnameAgnostic={originPathnameAgnostic}
  defaultPath="/d"
  i18nPropertyName="documentType"
  description="By Document Type"
/>
